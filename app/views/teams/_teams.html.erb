<% @teams.each do |team| %>
  <li><%= link_to team.name, team_path(team), method: :get %>
    <div style="display: inline-block; position: absolute; right: 25px;">
      <%= admin_panteam(team)[0] unless admin_panteam(team).nil? %>
      <%= admin_panteam(team)[1] unless admin_panteam(team).nil? %>
    </div>
  </li>
<% end %>