<% if @tournplr.empty? %>
  Нет заявленных игроков
<% else %>
  <% @tournplr.each do |player| %>
    <%= player.name %>
    <a data-toggle="tooltip" data-placement="top" title="" rel="nofollow" onClick="delClick(<%= player.id %>)">
      <i class="fa fa-close"></i>
    </a>
    <br>
  <% end %>
<% end %>